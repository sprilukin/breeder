    "spawn",
    "exec"
  ],
  "license": "MIT",
  "repository": "https://github.com/sprilukin/spawn-lord",
  "homepage": "https://github.com/sprilukin/spawn-lord",
  "author": "Sergey Prilukin <sprilukin@gmail.com>",
  "scripts": {
    "install": "node_modules/.bin/tsc -p tsconfig.prod.json",
